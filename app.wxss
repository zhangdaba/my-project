page, .page {
  height: 100%;
  font-family: 
  'PingFang SC',
  'Helvetica Neue',
   Helvetica,
  'Droid Sans Fallback',
  'Microsoft Yachei',
  sans-serif;
  font-size: 28rpx;
  background: #f6f7fb;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.first .box {
  padding-left: 20rpx;
  display: inline-block;
}

@font-face {
  font-family: 'iconfont';  /* project id 1147305 */
  src: url('//at.alicdn.com/t/font_1147305_ziw6uddwlh.eot');
  src: url('//at.alicdn.com/t/font_1147305_ziw6uddwlh.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1147305_ziw6uddwlh.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1147305_ziw6uddwlh.woff') format('woff'),
  url('//at.alicdn.com/t/font_1147305_ziw6uddwlh.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1147305_ziw6uddwlh.svg#iconfont') format('svg');
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 40rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

